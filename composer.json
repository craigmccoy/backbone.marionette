{
    "name": "craigmccoy/backbone.marionette",
    "description": "Backbone.Marionette is a composite application library for Backbone.js that aims to simplify the construction of large scale JavaScript applications.",
    "keywords": ["backbone", "plugin", "computed", "field", "model", "client", "browser"],
    "type": "component",
    "homepage": "https://github.com/marionettejs/backbone.marionette",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/marionettejs/backbone.marionette/issues"
    },
    "authors": [
        {
            "name": "Derick Bailey",
            "email": "marionettejs@gmail.com"
        }
    ],
    "require": {
        "robloach/component-installer": "*",
        "components/backbone": "1.1.*",
        "craigmccoy/backbone.wreqr": "1.0.*",
        "craigmccoy/backbone.babysitter": "0.1.*"
    },
    "extra": {
        "component": {
            "scripts": [
                "backbone.marionette.js"
            ],
            "files": [
                "backbone.marionette.min.js",
                "backbone.marionette.map"
            ],
            "shim": {
                "deps": [
                    "backbone",
		    "backbone.wreqr",
		    "backbone.babysitter"
                ],
                "exports": "Backbone.Marionette"
            }
        }
    }
}
